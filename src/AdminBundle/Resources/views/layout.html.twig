    <![endif]-->
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
</head>

<body>
<div id="wrapper">

{% include 'AdminBundle::nav_top.html.twig' %}
{% include 'AdminBundle::nav_side.html.twig' %}

{% block body %}{% endblock %}
{% block javascripts %}{% endblock %}
</div>

<script src="{{ asset('bundles/admin/js/jquery-1.10.2.js') }}"></script>
<!-- Bootstrap Js -->
<script src="{{ asset('bundles/admin/js/bootstrap.min.js') }}"></script>
<!-- Metis Menu Js -->
<script src="{{ asset('bundles/admin/js/jquery.metisMenu.js') }}"></script>
<!-- Morris Chart Js -->
<script src="{{ asset('bundles/admin/js/morris/raphael-2.1.0.min.js') }}"></script>
<script src="{{ asset('bundles/admin/js/morris/morris.js') }}"></script>
<!-- Custom Js -->
<script src="{{ asset('bundles/admin/js/custom-scripts.js') }}"></script>

</body>
</html>
